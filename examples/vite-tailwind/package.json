{
	"name": "vite-tailwind",
	"description": "vite-tailwind",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "vite --mode localhost",
		"clean": "rm -fr .coverage node_modules out package-lock.json",
		"make": "npm run clean && npm install",
		"build": "vite build --mode",
		"component": "portalx component",
		"container": "portalx container",
		"page": "portalx page",
		"eslint": "standard src",
		"test": "vitest",
		"test:name": "vitest --testNamePattern",
		"test:silent": "vitest --silent",
		"test:coverage": "vitest --silent --coverage"
	},
	"devDependencies": {
		"@tailwindcss/vite": "^4.0.0-beta.7",
		"@vitejs/plugin-react": "^4.3.4",
		"@vitest/coverage-v8": "^2.1.8",
		"autoprefixer": "^10.4.20",
		"jsdom": "^25.0.1",
		"tailwindcss": "^4.0.0-beta.7",
		"vite": "^6.0.3",
		"vitest": "^2.1.8"
	},
	"dependencies": {
		"portalx": "file:../../",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	}
}
